{
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-react"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": [true, "never-prefix"],
    "no-empty": false
  }
}
